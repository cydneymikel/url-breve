{
    "name": "url-breve",
    "version": "1.0.0",
    "description": "A simple URL shortener using Node.js, Express, and Supabase/Prisma.",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "prisma:migrate": "npx prisma migrate dev --name init",
        "test": "NODE_OPTIONS='--experimental-vm-modules' npx jest --silent",
        "test:watch": "NODE_OPTIONS='--experimental-vm-modules' npx jest --watch",
        "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' npx jest --coverage"
    },
    "keywords": [
        "nodejs",
        "express",
        "prisma",
        "supabase",
        "url-shortener"
    ],
    "author": "Cydney Auman",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^6.19.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0",
        "nanoid": "^5.1.6",
        "pino": "^10.1.0",
        "pino-http": "^11.0.0",
        "pino-pretty": "^13.1.2",
        "prisma": "^6.19.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@jest/globals": "^30.2.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.11",
        "supertest": "^7.1.4"
    }
}
